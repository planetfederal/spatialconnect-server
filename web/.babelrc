{
  "presets": ["es2015", "react"],
  "plugins": ["transform-object-rest-spread", "transform-class-properties"],
  "env": {
    "local": {
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": "react-transform-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }]
      }]]
    }
  }
}
